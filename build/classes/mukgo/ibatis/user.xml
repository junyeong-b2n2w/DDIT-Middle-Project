	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">
	
<sqlMap namespace="store">
	<select id="SearchStore" parameterClass="java.util.Map" resultClass="StoreVO">
